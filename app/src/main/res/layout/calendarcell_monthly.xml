<?xml version="1.0" encoding="utf-8"?>
<applusvelosi.projects.android.salt.utils.customviews.SquareGridItemView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_input_blur">

    <Button
        android:id="@+id/calendar_day_gridcell"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@android:color/transparent"
        android:textSize="@dimen/fsizes_listcell_header"
        android:textColor="@color/black"
        android:textStyle="bold">
    </Button>
    
    <LinearLayout
        android:id="@+id/container_mycalendar_cells_events"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:gravity="right"
        android:padding="5dp"/>

</applusvelosi.projects.android.salt.utils.customviews.SquareGridItemView>